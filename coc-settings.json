{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "Markdown",
    "ts",
    "typescript",
    "tsx",
    "json",
    "javascript",
    "tsx",
    "jsx",
    "java",
    "go"
  ],
  "java.format.enabled": true,
  "java.format.onType.enabled": true,
  "java.signatureHelp.enabled": true,
  "java.errors.incompleteClasspath.severity": "ignore",
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "filetypes": ["go"]
    }
  }
}
